{
  "name": "live-wallpaper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "biome check src",
    "format": "biome check --write src",
    "tauri": "tauri",
    "prepare": "husky",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "yarn pages:build && wrangler pages dev",
    "deploy": "yarn pages:build && wrangler pages deploy"
  },
  "lint-staged": {
    "src/*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --write --organize-imports-enabled=false --no-errors-on-unmatched"
    ]
  },
  "dependencies": {
    "@withgraphite/graphite-cli": "^1.4.8",
    "babel-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "clsx": "^2.1.1",
    "next": "^15.0.4",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/next-on-pages": "^1.13.7",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.7",
    "internal-ip": "^8.0.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "shadcn": "^2.1.7",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "wrangler": "^3.99.0"
  }
}
